{
  "name": "nevergreen",
  "version": "0.0.0",
  "description": "A build monitor with attitude",
  "private": true,
  "author": "Build Canaries",
  "homepage": "https://nevergreen.io",
  "license": "EPL-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/build-canaries/nevergreen.git"
  },
  "bugs": {
    "url": "https://github.com/build-canaries/nevergreen/issues"
  },
  "scripts": {
    "clean": "rm -rf resources/public",
    "build": "webpack --progress --mode=development",
    "build-prod": "webpack --mode=production",
    "test": "NODE_ENV=dev mocha --require @babel/polyfill --require ./test/client/Setup.js --require @babel/register -R mocha-circleci-reporter --reporter-options mochaFile=./target/client/test-results.xml --recursive test/client",
    "coverage": "nyc --reporter=html npm test",
    "lint": "npm run lintJs && npm run lintCss",
    "lintJs": "eslint ./src/client ./test/client",
    "lintCss": "stylelint \"src/client/**/*.scss\" --syntax scss",
    "watchJs": "watch 'npm run clean && npm run build' src/client",
    "watchTest": "watch 'npm run test' src/client test/client",
    "watchLint": "watch 'npm run lint' src/client test/client",
    "ci-stub-server": "node ci_stub_server/server",
    "generate-schema-validator": "ajv compile -s src/client/common/repo/schema.json -o src/client/common/repo/ValidateSchema.js --remove-additional --all-errors"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "bourbon-neat": "3.0.0",
    "classnames": "2.2.6",
    "clipboard": "2.0.1",
    "date-fns": "1.29.0",
    "history": "4.7.2",
    "immutable": "4.0.0-rc.10",
    "is-my-json-valid": "2.19.0",
    "localforage": "1.7.2",
    "lodash": "4.17.11",
    "memoize-one": "4.0.2",
    "modularscale-sass": "3.0.8",
    "mousetrap": "1.6.2",
    "normalize.css": "8.0.0",
    "project-name-generator": "2.1.5",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-immutable": "4.0.0",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.12.1",
    "semver": "5.6.0",
    "superagent": "3.8.3"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "ajv-cli": "3.0.0",
    "autoprefixer": "9.2.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "basic-auth-connect": "1.0.0",
    "browserslist": "4.2.1",
    "chai": "4.2.0",
    "chai-enzyme": "1.0.0-beta.1",
    "chai-immutable": "1.6.0",
    "copy-webpack-plugin": "4.5.3",
    "css-loader": "1.0.0",
    "dirty-chai": "2.0.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "eslint": "5.7.0",
    "eslint-plugin-babel": "5.2.1",
    "eslint-plugin-mocha": "5.2.0",
    "eslint-plugin-react": "7.11.1",
    "express": "4.16.4",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "ignore-styles": "5.0.1",
    "imagemin": "6.0.0",
    "imagemin-optipng": "5.2.1",
    "imagemin-svgo": "7.0.0",
    "img-loader": "3.0.0",
    "jsdom": "12.2.0",
    "lolex": "3.0.0",
    "mini-css-extract-plugin": "0.4.4",
    "mocha": "5.2.0",
    "mocha-circleci-reporter": "0.0.3",
    "node-sass": "4.9.4",
    "nyc": "13.1.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "proxyquire": "2.1.0",
    "raw-loader": "0.5.1",
    "react-test-renderer": "16.5.2",
    "resolve-url-loader": "3.0.0",
    "sass-loader": "7.1.0",
    "sinon": "7.0.0",
    "sinon-chai": "3.2.0",
    "style-loader": "0.23.1",
    "stylelint": "9.6.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "1.0.0",
    "stylelint-scss": "3.3.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.1.2",
    "watch": "1.0.2",
    "webpack": "4.21.0",
    "webpack-cli": "3.1.2",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-url": "7.0.0"
  }
}

{
  "name": "nevergreen",
  "version": "0.0.0",
  "description": "A build monitor with attitude",
  "private": true,
  "author": "Build Canaries",
  "homepage": "https://nevergreen.io",
  "license": "EPL-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/build-canaries/nevergreen.git"
  },
  "bugs": {
    "url": "https://github.com/build-canaries/nevergreen/issues"
  },
  "scripts": {
    "clean": "rm -rf resources/public",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "build": "webpack --progress --config webpack/dev.config.js",
    "build:prod": "webpack --config webpack/prod.config.js",
    "build:schema-validator": "ajv compile -s src/client/reducers/schema.json -o src/client/reducers/ValidateConfiguration.js --remove-additional --all-errors",
    "test": "NODE_ENV=dev mocha --require @babel/polyfill --require ./test/client/Setup.js --require @babel/register -R mocha-circleci-reporter --reporter-options mochaFile=./target/client/test-results.xml --recursive test/client",
    "test:coverage": "nyc --reporter=html npm test",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint ./src/client ./test/client",
    "lint:css": "stylelint \"src/client/**/*.scss\" --syntax scss",
    "watch:js": "npm run clean && webpack --watch --config webpack/dev.config.js",
    "watch:test": "watch 'npm run test' src/client test/client",
    "watch:lint": "watch 'npm run lint' src/client test/client",
    "start:ci-stub-server": "node ci_stub_server/server"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "bourbon-neat": "3.0.0",
    "classnames": "2.2.6",
    "clipboard": "2.0.4",
    "date-fns": "1.29.0",
    "history": "4.7.2",
    "immutable": "4.0.0-rc.12",
    "localforage": "1.7.3",
    "lodash": "4.17.11",
    "memoize-one": "4.0.3",
    "modularscale-sass": "3.0.8",
    "mousetrap": "1.6.2",
    "normalize.css": "8.0.1",
    "project-name-generator": "2.1.5",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-modal": "3.6.1",
    "react-redux": "5.1.1",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.7",
    "redux-immutable": "4.0.0",
    "redux-thunk": "2.3.0",
    "semver": "5.6.0",
    "superagent": "4.0.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "ajv-cli": "3.0.0",
    "autoprefixer": "9.3.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "basic-auth-connect": "1.0.0",
    "browserslist": "4.3.5",
    "chai": "4.2.0",
    "chai-enzyme": "1.0.0-beta.1",
    "chai-immutable": "1.6.0",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "1.0.1",
    "cypress": "3.1.2",
    "dirty-chai": "2.0.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "eslint": "5.9.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-mocha": "5.2.0",
    "eslint-plugin-react": "7.11.1",
    "express": "4.16.4",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "ignore-styles": "5.0.1",
    "imagemin": "6.0.0",
    "imagemin-optipng": "6.0.0",
    "imagemin-svgo": "7.0.0",
    "img-loader": "3.0.1",
    "jsdom": "13.0.0",
    "lolex": "3.0.0",
    "mini-css-extract-plugin": "0.4.5",
    "mocha": "5.2.0",
    "mocha-circleci-reporter": "0.0.3",
    "node-sass": "4.10.0",
    "nyc": "13.1.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "proxyquire": "2.1.0",
    "raw-loader": "0.5.1",
    "react-test-renderer": "16.6.3",
    "resolve-url-loader": "3.0.0",
    "sass-loader": "7.1.0",
    "sinon": "7.1.1",

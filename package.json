    "resolve-url-loader": "2.3.0",
    "sass-loader": "7.0.1",
    "sinon": "5.0.7",
    "sinon-chai": "3.0.0",
    "style-loader": "0.21.0",
    "stylelint": "9.2.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "0.8.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.0.1",
    "watch": "1.0.2",
    "webpack": "4.8.1",

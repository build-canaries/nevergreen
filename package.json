{
  "name": "nevergreen",
  "version": "0.0.0",
  "description": "A build monitor with attitude",
  "private": true,
  "author": "Build Canaries",
  "homepage": "https://nevergreen.io",
  "license": "EPL-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/build-canaries/nevergreen.git"
  },
  "bugs": {
    "url": "https://github.com/build-canaries/nevergreen/issues"
  },
  "scripts": {
    "clean": "rm -rf resources/public",
    "build": "webpack --progress --mode=development",
    "build-prod": "webpack --mode=production",
    "test": "NODE_ENV=dev mocha --require ./test/client/Setup.js --require babel-register -R mocha-circleci-reporter --reporter-options mochaFile=./target/client/test-results.xml --recursive test/client",
    "coverage": "nyc --reporter=html npm test",
    "lint": "npm run lintJs && npm run lintCss",
    "lintJs": "eslint ./src/client ./test/client",
    "lintCss": "stylelint src/client/*.scss src/client/**/*.scss src/client/**/**/*.scss src/client/**/**/**/*.scss --syntax scss",
    "watchJs": "watch 'npm run clean && npm run build' src/client",
    "watchTest": "watch 'npm run test' src/client test/client",
    "watchLint": "watch 'npm run lint' src/client test/client",
    "ci-stub-server": "node ci_stub_server/server"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bourbon-neat": "2.0.0",
    "classnames": "2.2.5",
    "clipboard": "2.0.1",
    "date-fns": "1.29.0",
    "history": "4.7.2",
    "immutable": "3.8.2",
    "is-my-json-valid": "2.17.2",
    "localforage": "1.7.1",
    "lodash": "4.17.10",
    "modularscale-sass": "3.0.5",
    "mousetrap": "1.6.1",
    "normalize.css": "8.0.0",
    "project-name-generator": "2.1.4",
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "4.0.0",
    "redux-immutable": "4.0.0",
    "redux-thunk": "2.2.0",
    "semver": "5.5.0",
    "superagent": "3.8.3"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "basic-auth-connect": "1.0.0",
    "chai": "4.1.2",
    "chai-enzyme": "1.0.0-beta.0",
    "chai-immutable": "1.6.0",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.11",
    "dirty-chai": "2.0.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-react": "7.7.0",
    "express": "4.16.3",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "ignore-styles": "5.0.1",
    "imagemin": "5.3.1",
    "imagemin-optipng": "5.2.1",
    "imagemin-svgo": "6.0.0",
    "img-loader": "3.0.0",
    "jsdom": "11.10.0",
    "lolex": "2.3.2",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "5.1.1",
